{
  "buildCommand": "pip install --no-deps -r requirements.txt",
  "framework": "django",
  "builds": [
    {
      "src": "learn/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11.5" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "learn/wsgi.py"
    }
  ]
}

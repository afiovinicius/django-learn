{
  "builds": [
    {
      "buildCommand": "pip install --no-deps -r requirements.txt",
      "src": "learn/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.10.6" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "learn/wsgi.py"
    }
  ]
}
